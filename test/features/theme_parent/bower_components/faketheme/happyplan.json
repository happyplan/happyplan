{
  "theme": {
    "faketheme": {
      "parent": "default",
      "path": {
        "_": "<%= happyplan.bower_components %>/faketheme/src",
        "assets": {
          "_": "<%= happyplan.theme.faketheme.path._ %>/assets",
          "styles": "<%= happyplan.theme.faketheme.path.assets._ %>/_styles",
          "images": "<%= happyplan.theme.faketheme.path.assets._ %>/_images",
          "glyphicons": "<%= happyplan.theme.faketheme.path.assets._ %>/_glyphicons",
          "fonts": "<%= happyplan.theme.faketheme.path.assets._ %>/fonts"
        },
        "html": {
          "_": "<%= happyplan.theme.faketheme.path._ %>",
          "layouts": "<%= happyplan.theme.faketheme.path.html._ %>/_layouts",
          "partials": "<%= happyplan.theme.faketheme.path.html._ %>/_partials"
        }
      },
      "assets": {
        "styles": [{
          "src": "<%= happyplan.theme.faketheme.path.assets.styles %>/faketheme.scss",
          "dest": "<%= happyplan.path.dist.assets.styles %>/faketheme.css"
        }]
      }
    }
  },
  "jekyll": {
    "permalink": "/:categories/:year/:title.html"
  }
}
